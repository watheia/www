{
  "version": 2,
  "projects": {
    "designx": {
      "root": "libs/designx",
      "sourceRoot": "libs/designx/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/designx",
            "tsConfig": "libs/designx/tsconfig.lib.json",
            "project": "libs/designx/package.json",
            "entryFile": "libs/designx/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/designx/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/designx/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/designx"],
          "options": {
            "jestConfig": "libs/designx/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "layout": {
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/layout",
            "tsConfig": "libs/layout/tsconfig.lib.json",
            "project": "libs/layout/package.json",
            "entryFile": "libs/layout/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/layout/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/layout/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/layout"],
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "model": {
      "root": "libs/model",
      "sourceRoot": "libs/model/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/model/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/model"],
          "options": {
            "jestConfig": "libs/model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "portal": {
      "root": "apps/portal",
      "sourceRoot": "apps/portal",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/portal",
            "outputPath": "dist/apps/portal"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "portal:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "portal:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "portal:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/portal"],
          "options": {
            "jestConfig": "apps/portal/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portal/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "portal-e2e": {
      "root": "apps/portal-e2e",
      "sourceRoot": "apps/portal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portal-e2e/cypress.json",
            "tsConfig": "apps/portal-e2e/tsconfig.e2e.json",
            "devServerTarget": "portal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portal-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "theme": {
      "root": "libs/theme",
      "sourceRoot": "libs/theme/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/theme",
            "tsConfig": "libs/theme/tsconfig.lib.json",
            "project": "libs/theme/package.json",
            "entryFile": "libs/theme/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/theme/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/theme/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/theme"],
          "options": {
            "jestConfig": "libs/theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "portal"
}
